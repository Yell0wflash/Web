        .btn-login {
            width: 100%;
            background-color: darkred;
            border-color: darkred;
            border-radius: 100px;
        }

        .btn-login:hover {
            background-color: red;
            border-color: red;
        }

        .input-group-text{ 
            background-color: #212226;
            color: white;
        }

        .form-control:valid {
            background-color: #212226;
            color: white;
        }

        .form-control:invalid {
            background-color: #2f0001;
            color: white;
        }

          .nav-color {
            background-color: #212226;
        }

